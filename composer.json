{
    "name": "malzariey/production-debugbar",
    "description": "Use Debugbar in Production for performance analysis and fast debugging",
    "keywords": [
        "Majid Al Zariey",
        "laravel",
        "production-debugbar"
    ],
    "homepage": "https://github.com/malzariey/production-debugbar",
    "license": "MIT",
    "authors": [
        {
            "name": "Majid Al Zariey",
            "email": "malzariey@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "spatie/laravel-package-tools": "^1.16",
        "laravel/laravel": "v10.0 | ^11.0 || ^12.0",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "ext-json": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "laravel/pint": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "malzariey\\ProductionDebugbar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {

    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "malzariey\\ProductionDebugbar\\ProductionDebugbarServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
